batch_connect:
  template: vnc
script:
  reservation_id: <%= custom_reservation %>
  email: <%= custom_email_address %>
  native:
    - "--mem=<%= custom_memory_per_node.blank? ? 2 : custom_memory_per_node.to_s %>G"
    - "--cpus-per-task=<%= custom_num_cores.blank? ? 1 : custom_num_cores.to_i %>"

